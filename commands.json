{
  "memory": "free",
  "disk": "df -h",
  "ip": "ifconfig",
  "ports": "netstat -lu && netstat -lt",
  "users": "lastb",
  "process": "ps -aux | tail --lines=10",
  "table": "netstat -nr",
  "dhcp": "cat /etc/dhcp/dhcpd.conf | grep group -A 100",
  "dns": "cat /etc/bind/named.conf.local",
  "web": "apache2ctl -S",
  "snmp": "snmpget -v1 -c public 192.168.10.1 sysDescr.0",
  "client": "hostname"
}
